
@EXPLORE_EVENT_1(event_ID, method)
    #DIM event_id
    #DIMS method
    SELECTCASE method
        ;应该是类似备注的东西?
        CASE "描述"
            VARSET RESULTS
            RESULTS:0 = 这是一个测试事件
        CASE "读取"
            ;事件名
            EXPLORE_EVENT_OBJ:event_id:INDEX_EXPLORE_EVENT("名称")    = 测试事件2
            EXPLORE_EVENT_OBJ:event_id:INDEX_EXPLORE_EVENT("事件类型") = 固定事件
            EXPLORE_EVENT_OBJ:event_id:INDEX_EXPLORE_EVENT("描述") = 前方有座石雕
        CASEELSE
    ENDSELECT
    RETURN
    